<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="835.0" prefWidth="1535.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.digikala.ShopPage">
   <children>
      <ButtonBar layoutX="1321.0" layoutY="14.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="backID" mnemonicParsing="false" onMouseClicked="#onBackBottomClick" text="Back" />
            <Button fx:id="closeID" mnemonicParsing="false" onMouseClicked="#onCloseBottomClick" text="Close" />
        </buttons>
      </ButtonBar>
      <ToolBar layoutX="14.0" layoutY="14.0" orientation="VERTICAL" prefHeight="104.0" prefWidth="75.0">
        <items>
            <Button fx:id="sortID" mnemonicParsing="false" onMouseClicked="#onSortBottomClick" prefHeight="24.0" prefWidth="73.0" text="Sort" textFill="#644ab5">
               <font>
                  <Font name="Sylfaen" size="18.0" />
               </font>
            </Button>
            <MenuButton fx:id="filterID" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#onFilterBottomClick" prefHeight="28.0" prefWidth="73.0" text="Filter" textFill="#644ab5">
              <items>
                <MenuItem fx:id="byCategoryID" mnemonicParsing="false" onMenuValidation="#onCategory" text="by category" />
                <MenuItem fx:id="byPrice" mnemonicParsing="false" onMenuValidation="#onPrice" text="by range price" />
                  <MenuItem fx:id="byAvailabilityID" mnemonicParsing="false" onMenuValidation="#onAvailability" text="by availability" />
                  <MenuItem fx:id="byScore" mnemonicParsing="false" onMenuValidation="#onScore" text="by score" />
                  <MenuItem fx:id="byDigitalID" mnemonicParsing="false" onMenuValidation="#onDigital" text="by digital" />
                  <MenuItem fx:id="byStationery" mnemonicParsing="false" onMenuValidation="#on" text="by stationery" />
                  <MenuItem fx:id="byVehicleID" mnemonicParsing="false" onMenuValidation="#onVehicle" text="by vehicle" />
                  <MenuItem fx:id="byFood" mnemonicParsing="false" onMenuValidation="#onFood" text="by food" />
              </items>
               <font>
                  <Font name="Sylfaen" size="14.0" />
               </font>
            </MenuButton>
            <MenuButton fx:id="searchID" mnemonicParsing="false" onMouseClicked="#onSearchClicked" text="Search" textFill="#644ab5">
              <items>
                <MenuItem fx:id="byNameID" mnemonicParsing="false" onMenuValidation="#onByNameValidation" text="by name" />
                <MenuItem fx:id="byIDID" mnemonicParsing="false" onAction="#onByIDClicked" text="by product ID" />
                  <MenuItem mnemonicParsing="false" text="Unspecified Action" />
              </items>
               <font>
                  <Font name="Sylfaen" size="14.0" />
               </font>
            </MenuButton>
        </items>
      </ToolBar>
      <ButtonBar layoutX="636.0" layoutY="781.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="preID" mnemonicParsing="false" onMouseClicked="#onPreBottomClick" text="Pre" />
            <Button fx:id="nextID" mnemonicParsing="false" onMouseClicked="#onNextBottomClick" text="Next" />
        </buttons>
      </ButtonBar>
      <AnchorPane layoutX="181.0" layoutY="66.0" prefHeight="670.0" prefWidth="1140.0">
         <children>
            <AnchorPane fx:id="one" layoutX="14.0" layoutY="14.0" onMouseClicked="#onOneClicked" prefHeight="222.0" prefWidth="200.0" />
            <AnchorPane fx:id="two" layoutX="246.0" layoutY="14.0" onMouseClicked="#onTwoClicked" prefHeight="222.0" prefWidth="200.0" />
            <AnchorPane fx:id="three" layoutX="470.0" layoutY="25.0" onMouseClicked="#onThreeClicked" prefHeight="214.0" prefWidth="200.0" />
            <AnchorPane fx:id="four" layoutX="640.0" layoutY="20.0" onMouseClicked="#onFourClicked" prefHeight="214.0" prefWidth="200.0" />
            <AnchorPane fx:id="five" layoutX="853.0" layoutY="20.0" onMouseClicked="#onFiveClicked" prefHeight="214.0" prefWidth="200.0" />
            <AnchorPane fx:id="six" layoutX="32.0" layoutY="280.0" onMouseClicked="#onSixClicked" prefHeight="252.0" prefWidth="200.0" />
            <AnchorPane fx:id="seven" layoutX="240.0" layoutY="284.0" onMouseClicked="#onSevenClicked" prefHeight="252.0" prefWidth="200.0" />
            <AnchorPane fx:id="eight" layoutX="470.0" layoutY="294.0" onMouseClicked="#onEightClicked" prefHeight="252.0" prefWidth="200.0" />
            <AnchorPane fx:id="nine" layoutX="688.0" layoutY="306.0" onMouseClicked="#onNineClicked" prefHeight="252.0" prefWidth="200.0" />
            <AnchorPane layoutX="866.0" layoutY="300.0" prefHeight="200.0" prefWidth="200.0" />
            <AnchorPane fx:id="ten" layoutX="876.0" layoutY="310.0" onMouseClicked="#onTenClicked" prefHeight="232.0" prefWidth="200.0" />
         </children>
      </AnchorPane>
      <Text fill="#644ab5" layoutX="662.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Shabnam's Shop!">
         <font>
            <Font name="Sylfaen" size="36.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
